<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
xmlns:p = "http://www.springframework.org/schema/p"
xmlns:context = "http://www.springframework.org/schema/context"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd
   ">
   
   <context:annotation-config/>
   
   <bean id="faculty" class="com.shivam.SpringDemoCdac.xmlAutowire.Faculty"
   p:name="Shivam Shankhdhar" p:expertise="Web Developement" />
   
   <bean id="faculty1" class="com.shivam.SpringDemoCdac.xmlAutowire.Faculty"
   p:name="Shraddha Shankhdhar" p:expertise="Artificial Intelligence" />
   
   <bean id="course1" class="com.shivam.SpringDemoCdac.xmlAutowire.Course"
   p:name="Web Developement" p:duration="6" />
   
    <bean id="course2" class="com.shivam.SpringDemoCdac.xmlAutowire.Course"
   p:name="Artificial Intelligence" p:duration="24"  />
   
   
   
    </beans>
    